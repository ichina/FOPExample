workspace 'FOPExample'
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, "10.0"


target 'FOPExample' do
    project 'FOPExample.xcodeproj'
    use_frameworks!
    pod 'RxSwift'
    pod 'RealmSwift'
    pod 'Alamofire'
    pod 'Swinject'
end

target 'FOPFoundation' do
    project 'FOPFoundation/FOPFoundation.xcodeproj'
    use_frameworks!
    pod 'RealmSwift'
    pod 'Swinject'
end

target 'ChatFeature' do
    project 'ChatFeature/ChatFeature.xcodeproj'
    use_frameworks!
    pod 'Alamofire'
    pod 'Swinject'
end

target 'CallFeature' do
    project 'CallFeature/CallFeature.xcodeproj'
    use_frameworks!
    pod 'RealmSwift'
    pod 'Swinject'
end




